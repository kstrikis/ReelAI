# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Firebase Project ID (optional, will use default from firebase-admin)
PROJECT_ID=your_project_id

# Environment (development/production)
NODE_ENV=development

# LangSmith Tracing (for debugging, monitoring, and evaluating LangChain applications)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key
LANGCHAIN_PROJECT=reelai-story-generation  # Your project name

# Optional: Reduce tracing latency in non-serverless environments
# LANGCHAIN_CALLBACKS_BACKGROUND=false  # Keep false for Firebase Functions 

# ElevenLabs API Key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# AIMLAPI API Key
AIMLAPI_API_KEY=your_aimlapi_api_key 